<div class="center jumbotron">
  <% if logged_in? %>
    <p>ログイン完了ユーザ: <%= current_user.name %></p>
    
    <%= render 'tasks/tasks', tasks:@tasks %>
      


  <%= link_to '新規タスク作成', new_task_path %>
  <% else %>
    <div class="text-center">
      <h1>Tasklist</h1>
      <%= link_to 'Sign up now!', signup_path %>
      <%= link_to 'Login', login_path %>
    </div>
  <% end %>
</div>